{
  "name": "organism-react-iframe",
  "version": "0.3.0",
  "description": "Rendering to iFrames in React",
  "repository": "react-atomic/react-atomic-organism",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "create-el": "*",
    "css-query-selector": "*",
    "exec-script": "^0.2.x",
    "get-object-value": "*",
    "getoffset": "*",
    "react-atomic-molecule": "*",
    "reshow-runtime": "*",
    "setimmediate": "^1.0.5",
    "smooth-scroll-to": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "react": "^16.x",
    "react-dom": "^16.x"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=build babel src -d build/cjs/src --root-mode upward && BABEL_ENV=build babel ui -d build/cjs/ui --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --root-mode upward && BABEL_ENV=es babel ui -d build/es/ui --root-mode upward",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
