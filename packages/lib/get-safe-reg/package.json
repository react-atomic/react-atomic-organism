{
  "name": "get-safe-reg",
  "version": "0.3.0",
  "description": "get safe regular-expression",
  "repository": "react-atomic/react-atomic-organism",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.x",
    "chai": "*",
    "mocha": "*"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs/src --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --root-mode upward",
    "mochaFor": "mocha",
    "mocha": "npm run mochaFor -- 'build/cjs/**/__tests__/*.js'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build/cjs",
    "build/es",
    "package.json",
    "README.md"
  ]
}
