{
  "version": "0.4.3",
  "name": "organism-react-tag-input",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-atomic/react-atomic-organism",
    "directory": "packages/organism-react-tag-input"
  },
  "description": "React Tag Input",
  "keywords": [
    "react"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "array.dedup": "*",
    "call-func": "*",
    "get-object-value": "*",
    "react-atomic-molecule": "*",
    "react-atomic-organism": "*",
    "reshow-flux": "*",
    "reshow-runtime": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "react": "^18.x",
    "react-dom": "^18.x",
    "reshow-unit": "*"
  },
  "exports": {
    "require": "./build/cjs/src/index.js",
    "import": "./build/es/src/index.mjs"
  },
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.mjs",
  "scripts": {
    "update-compile-sh": "yo reshow:compile-sh",
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "BABEL_ENV=cjs babel src -d build/cjs/src --ignore /**/__tests__ --root-mode upward",
    "build:es": "BABEL_ENV=es babel src -d build/es/src --out-file-extension .mjs --root-mode upward",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "STRICT_MODE=on mocha -r global-jsdom/register",
    "mocha": "npm run mochaFor -- 'build/es/**/__tests__/*.mjs'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
