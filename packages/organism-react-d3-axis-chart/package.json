{
  "name": "organism-react-d3-axis-chart",
  "version": "0.4.2",
  "description": "D3 Axis Chart",
  "repository": "react-atomic/react-atomic-organism",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "call-func": "*",
    "d3-lib": "*",
    "getoffset": "*",
    "organism-react-graph": "*",
    "react-atomic-molecule": "*",
    "reshow-hooks": "*",
    "to-percent-js": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "reshow-unit": "*"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs:src": "BABEL_ENV=cjs babel src -d build/cjs/src --root-mode upward",
    "build:cjs:ui": "BABEL_ENV=cjs babel ui -d build/cjs/ui --root-mode upward",
    "build:cjs": "npm run build:cjs:src && npm run build:cjs:ui",
    "build:es:src": "BABEL_ENV=es babel src -d build/es/src --root-mode upward --ignore /**/__tests__",
    "build:es:ui": "BABEL_ENV=es babel ui -d build/es/ui --root-mode upward --ignore /**/__tests__",
    "build:es": "npm run build:es:src && npm run build:es:ui",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "mocha -r jsdom-global/register",
    "mocha": "npm run mochaFor -- 'build/cjs/**/__tests__/*.js'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
