{
  "name": "organism-react-i13n",
  "version": "0.15.4",
  "description": "React I13N",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-atomic/react-atomic-organism",
    "directory": "packages/organism-react-i13n"
  },
  "homepage": "https://github.com/react-atomic/react-atomic-organism/tree/main/packages/organism-react-i13n",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "get-random-id": "*",
    "i13n": "*",
    "organism-react-ajax": "*",
    "organism-react-iframe": "*",
    "react-atomic-molecule": "*",
    "reshow-runtime": "*",
    "call-func": "*",
    "win-doc": "*",
    "window-onload": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.x",
    "chai": "*",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "jsdom": "*",
    "jsdom-global": "*",
    "mocha": "*",
    "react": "^16.x",
    "react-dom": "^16.x",
    "react-test-renderer": "*",
    "reshow": "*"
  },
  "scripts": {
    "clean": "find ./build -name '*.*' | xargs rm -rf",
    "build:cjs": "npm run build:cjs:src && npm run build:cjs:ui",
    "build:cjs:src": "BABEL_ENV=cjs babel src -d build/cjs/src --root-mode upward",
    "build:cjs:ui": "BABEL_ENV=cjs babel ui -d build/cjs/ui --root-mode upward",
    "build:es": "npm run build:es:src && npm run build:es:ui",
    "build:es:src": "BABEL_ENV=es babel src -d build/es/src --root-mode upward",
    "build:es:ui": "BABEL_ENV=es babel ui -d build/es/ui --root-mode upward",
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "mochaFor": "mocha -r jsdom-global/register",
    "mocha": "npm run mochaFor -- 'build/cjs/**/__tests__/*.js'",
    "test": "npm run build && npm run mocha",
    "prepublishOnly": "npm run test"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
